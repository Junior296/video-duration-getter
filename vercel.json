{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/duration",
      "methods": ["POST"],
      "dest": "server.js"
    },
    {
      "src": "/duration/web", 
      "methods": ["GET"],
      "dest": "server.js"
    },
    {
      "src": "/", 
      "dest": "/duration/web"
    }
  ]
}
